{
  "extends": ["next"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-extra-boolean-cast": "error",
    "no-empty": "error",
    "no-empty-function": "error",
    "no-useless-escape": "error",
    "react/display-name": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
